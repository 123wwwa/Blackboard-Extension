{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    ".emotion.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "**/*.tsx",
    "contents/timetable/index.tsxle.tsx",
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ],
      "types": ["@emotion/react/types/css-prop"],
		  "jsxImportSource": ["@emotion/react"]
    },
    "baseUrl": "."
  }
}