{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx",
    "contents/injectMembership.js"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ],
      "types": [
        "@emotion/react/types/css-prop"
      ],
      "jsxImportSource": ["@emotion/react"]
    },
    "baseUrl": "."
  }
}